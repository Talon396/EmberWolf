MODULE Target;
    ASM "string.s";
    EXTERN PROCEDURE memset, memcpy: CHAR;
    ASM "fwcalls.s";

    PROCEDURE TargetInit();
    BEGIN
        PUTINT(0B0000000H,1); (* Switch Framebuffer to 8-bit 1024x768 *)
        PUTINT(0B0000004H,0);
    END;

    PROCEDURE Halt();
    BEGIN
        WHILE 1 DO END;
    END;

END.